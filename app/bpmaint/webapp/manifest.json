{
    "_version": "1.32.0",
    "sap.app": {
        "id": "bpmaint",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainService": {
                "uri": "/../bp/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "offline": false,
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "ui5template.fiorielements.v4.lrop",
            "version": "1.0.0"
        }
    },  
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "1.97.0",
            "libs": {
                "sap.ui.core": {},
                "sap.fe.templates": {}
            }
        },
        "models": {
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "BusinessPartnerList",
                    "target": "BusinessPartnerList"
                },
                {
                    "pattern": "BusinessPartner({key}):?query:",
                    "name": "BusinessPartnerObjectPage",
                    "target": "BusinessPartnerObjectPage"
                },
                    {
                        "pattern": "BusinessPartner({key})/to_BusinessPartnerAddress({key2}):?query:",
                        "name": "BusinessPartnerAddressObjectPage",
                        "target": "BusinessPartnerAddressObjectPage"
                    }
            ],
            "targets": {
                "BusinessPartnerList": {
                    "type": "Component",
                    "id": "BusinessPartnerList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "BusinessPartner",
                            "variantManagement": "Page",
                            "navigation": {
                                "BusinessPartner": {
                                    "detail": {
                                        "route": "BusinessPartnerObjectPage"
                                    }
                                }
                            }
                        }
                    }
                },
                "BusinessPartnerObjectPage": {
                    "type": "Component",
                    "id": "BusinessPartnerObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "BusinessPartner",
                                "navigation": {
                                    "to_BusinessPartnerAddress": {
                                        "detail": {
                                            "route": "BusinessPartnerAddressObjectPage"
                                        }
                                    }
                                }
                        }
                    }
                },
                    "BusinessPartnerAddressObjectPage": {
                        "type": "Component",
                        "id": "BusinessPartnerAddressObjectPage",
                        "name": "sap.fe.templates.ObjectPage",
                        "options": {
                            "settings": {
                                "editableHeaderContent": false,
                                "entitySet": "BusinessPartnerAddress"
                            }
                        }
                    }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.platform.abap": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.platform.hcp": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [],
        "archeType": "transactional"
    }
}
